@model IEnumerable<CalculadoraPrestamo.Models.Interes>

<h1>Administrador de Tasa de Interes </h1>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.RangoInferior)</th>
            <th>@Html.DisplayNameFor(model => model.RangoSuperior)</th>
            <th>@Html.DisplayNameFor(model => model.TasaInteres)</th>
            <th>@Html.DisplayNameFor(model => model.Credito.Tipo)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => item.RangoInferior)</td>
                <td>@Html.DisplayFor(m => item.RangoSuperior)</td>
                <td>@Html.DisplayFor(m => item.TasaInteres)</td>
                <td>@Html.DisplayFor(m => item.Credito.Tipo)</td>
                <td>
                </td>
            </tr>
        }

    </tbody>

</table>